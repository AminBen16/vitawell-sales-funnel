{
  "name": "vitablog-platform",
  "version": "1.0.0",
  "description": "Professional full-stack blog platform for posting products and opportunities",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "prestart": "npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "init": "node init.js",
    "verify": "node verify-setup.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/migrate.js && node scripts/seed.js",
    "setup": "npm run init && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "blog",
    "products",
    "opportunities",
    "vitawell"
  ],
  "author": "VitaWell Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "nodemon": "^3.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.2",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1"
  }
}
