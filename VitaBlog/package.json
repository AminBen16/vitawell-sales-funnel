{
  "name": "vitablog-platform",
  "version": "1.0.0",
  "description": "Professional full-stack blog platform for posting products and opportunities",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "prestart": "npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "keywords": ["blog", "products", "opportunities", "vitawell"],
  "author": "VitaWell Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.2",
    "sequelize": "^6.35.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1"
  }
}
